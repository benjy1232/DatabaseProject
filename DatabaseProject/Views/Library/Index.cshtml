@model DatabaseProject.Controllers.LibraryController

@{
    Layout = "_Layout";
    ViewData["Title"] = "Music Library";
}

<!DOCTYPE html>

<html>
<head>
    <title>Music Library</title>
</head>
<body>
<div>
    <p>
        Welcome to this large Music Database that has no checks on what you are able to destroy with it :))
        <table>
            <tr>
                <th>Song</th>
                <th>Album</th>
                <th>Release Year</th>
                <th>Artist</th>
                <th>Album Artist</th>
                <th>Genre</th>
            </tr>
            @Html.Raw(ViewBag.Table)
        </table>
    </p>
</div>
</body>
</html>